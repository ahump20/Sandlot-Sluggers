    "@babylonjs/loaders": "^7.31.0",
    "@babylonjs/materials": "^7.31.0",
    "@babylonjs/havok": "^1.3.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "wrangler": "^3.87.0"
  }
}
